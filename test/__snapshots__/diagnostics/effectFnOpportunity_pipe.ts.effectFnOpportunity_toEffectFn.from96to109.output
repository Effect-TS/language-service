// code fix effectFnOpportunity_toEffectFn  output for range 96 - 109
import * as Effect from "effect/Effect"
import * as Runtime from "effect/Runtime"

export const shouldTrigger = Effect.fn("shouldTrigger")(function* <A, E, R>(effect: Effect.Effect<A, E, R>) {
    const runtime = yield* Effect.runtime<R>()
    Runtime.runSync(runtime)(effect)

    return yield* effect.pipe(Effect.ensuring(Effect.logFatal("Hello")))
}, Effect.withSpan("shouldTrigger"))
