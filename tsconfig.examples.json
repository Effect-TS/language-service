{
  "extends": "./tsconfig.base.json",
  "include": [
    "examples"
  ],
  "exclude": [
    "examples/completions/**.ts",
    "examples/gen-block/**.ts"
  ],
  "references": [
    {
      "path": "./tsconfig.src.json"
    }
  ],
  "compilerOptions": {
    "tsBuildInfoFile": ".tsbuildinfo/examples.tsbuildinfo",
    "rootDir": "examples",
    "noEmit": true,
    "noUnusedLocals": false,
    "strict": true,
    "paths": {
      "@/*": [
        "./examples/*"
      ]
    }
  }
}
