{
  "extends": "./tsconfig.base.json",
  "include": ["examples"],
  "references": [
    {
      "path": "./tsconfig.src.json"
    }
  ],
  "compilerOptions": {
    "tsBuildInfoFile": ".tsbuildinfo/examples.tsbuildinfo",
    "rootDir": "examples",
    "noEmit": true,
    "noUnusedLocals": false,
    "strict": true,
    "paths": {
      "@/*": ["./examples/*"]
    }
  }
}
