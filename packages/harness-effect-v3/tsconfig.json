{
  "compilerOptions": {
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "paths": {
      "@/*": ["./examples/*"]
    },
    "plugins": [
      {
        "name": "@effect/language-service",
        "namespaceImportPackages": [
          "effect"
        ]
      }
    ]
  },
  "exclude": ["examples/completions"]
}
