{
  "compilerOptions": {
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "paths": {
      "@/*": ["./examples/*"]
    }
  },
  "include": ["examples/**/*"],
  "exclude": ["examples/completions"]
}
